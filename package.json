{
  "name": "cap-po-approval",
  "version": "1.0.0", 
  "private": true,
  "scripts": {
    "start": "cds-serve",
    "watch": "cds watch",
    "build": "cds build"
  },
  "dependencies": {
    "@cap-js/sqlite": "^2.0.0",
    "@sap-cloud-sdk/connectivity": "^3.21.0",
    "@sap-cloud-sdk/http-client": "^3.21.0",
    "@sap-cloud-sdk/resilience": "^3.21.0",
    "@sap-cloud-sdk/util": "^3.21.0",
    "@sap/cds": "^9.2.0",
    "@sap/xssec": "^4.2.1",
    "passport": "^0.7.0",
    "express": "^4.21.2",
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=20"
  },
  "cds": {
    "requires": {
      "auth": {
        "[production]": {
          "kind": "xsuaa"
        },
        "[development]": {
          "kind": "mocked"
        }
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        }
      },
      "S4HANA_DEV": {
        "kind": "odata-v2",
        "credentials": {
          "destination": "S4HANA_DEV"
        }
      }
    },
    "build": {
      "target": "gen"
    }
  }
}